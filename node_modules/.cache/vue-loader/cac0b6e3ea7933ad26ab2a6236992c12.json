{ title }}</v-toolbar-title>\n        </v-toolbar>\n        <v-card-text v-show=\"!!message\">{{ message }}</v-card-text>\n        <v-flex xs12 class=\"email\" color=\"#f44336\">\n          <v-text-field v-model=\"email\" type=\"email\" label=\"Sähköposti*\" required></v-text-field>\n        <v-alert class=\"warning\" color=\"orange\" :value=\"this.inputAlert\" type=\"warning\">Tarkista osoite.</v-alert>\n        </v-flex>\n        <v-card-actions class=\"pt-0\">\n          <v-spacer></v-spacer>\n          <v-btn color=\"primary darken-1\" flat=\"flat\" @click.native=\"agree\">Jaa</v-btn>\n          <v-btn color=\"grey\" flat=\"flat\" @click.native=\"cancel\">Peruuta</v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </div>\n</template>\n\n<script>\nexport default {\n  data: () => ({\n    dialog: false,\n    resolve: null,\n    reject: null,\n    message: null,\n    title: null,\n    email: null,\n    inputAlert: false,\n    reg: /^(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,24}))$/,\n    options: {\n      color: \"primary\",\n      width: 320,\n      zIndex: 300\n    }\n  }),\n  methods: {\n    open(title, message, options) {\n      this.dialog = true;\n      this.title = title;\n      this.message = message;\n      this.options = Object.assign(this.options, options);\n      return new Promise((resolve, reject) => {\n        this.resolve = resolve;\n        this.reject = reject;\n      });\n    },\n    agree() {\n      if (this.validateEmail(this.email)) {\n        this.resolve(true);\n        this.dialog = false;\n      } else {\n        this.inputAlert = true;\n        setTimeout(() => {this.inputAlert = false}, 1000);\n      }\n    },\n    cancel() {\n      this.resolve(false);\n      this.dialog = false;\n    },\n    validateEmail(email) {\n      var re = /^(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\n      return re.test(String(email).toLowerCase());\n    }\n  }\n};\n</script>\n\n<style scoped>\n.email {\n  padding: 15px;\n}\n.warning {\n  padding: 3px;\n  font-size: 15px;\n  margin: auto;\n  border-radius: 5px;\n}\n</style>\n"]}]}